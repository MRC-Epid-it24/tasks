    "form-data": "^3.0.0",
    "json2csv": "^5.0.1",
    "mssql": "^6.2.0",
    "node-cron": "^2.0.3",
    "pg": "^8.2.0",
    "rimraf": "^3.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
